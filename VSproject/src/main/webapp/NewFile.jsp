/*=======================================================================
 내      용  : 메인 컨텐츠 더보기
 작  성  자  : judy
2017.11.23 - 최초생성
2017.00.00 - judy 수정
========================================================================*/	
function getAllList() {     
	$.ajax({
		type: 'POST',
		url: '/ajax/selectMainContentsList',
		data: {'nextLink':$("input[name=nextLink]").val(),'type':$("input[name=sortType]").val()},
		dataType: 'json',
		success: function(data){
			if(data==null){
				data = 0;
			}
			for(var i=0; i<data.length-1;i++){
				var objRow = $("#content-div").clone();
				objRow.attr("name", "card-container");  //지우거나 목록 전체의 선택을 위해 Name 준다.
				objRow.removeAttr("id");
				objRow.find("img:eq(0)").attr("src","/common/img/"+data[i]['Grade']+"Live.png");
				objRow.find("img:eq(1)").attr("src","https://s3.ap-northeast-2.amazonaws.com/avsbfile/"+data[i]['ImagePathA']);
				objRow.find("span:eq(0)").html(data[i]['NameA'] + " vs " + data[i]['NameB']);
				objRow.find("span:eq(1)").html(data[i]['Title']);
				//Row 클릭이벤트
				objRow.data('seq', data[i]['Seq']);
				objRow.click(function () {
					goViewContent($(this).data('seq'));
				}); 
				objRow.show();
				$("#all-div").append(objRow);			
			} 
			$("input[name=nextLink]").val(data[data.length-1]['nextLink']);
			$("input[name=curPage]").val(data[data.length-1]['curPage']);			
			if($("input[name=nextLink]").val() == 0){
				$("#aMoreALL").hide();
			}
			if($("input[name=nextLink]").val() > 0){
				$("#aMoreALL").show();
			}
		}
	});
}
